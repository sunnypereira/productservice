<?xml version="1.0" encoding="UTF-8"?>

<Configuration monitorInterval="60" status="WARN" >
	<Appenders>
		<Console name="Console-Appender" target="SYSTEM_OUT">
			<PatternLayout>
				<pattern>
                <!-- [%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} %c{10} - %msg%n -->
                	%d{ISO8601} [%t] [%-5level] %c{30} - %msg%n
                </pattern>>
			</PatternLayout>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console-Appender"/>
		</Root>
	</Loggers>
</Configuration>